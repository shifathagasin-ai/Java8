import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
        int num;
        do {
            num = sc.nextInt();
            
            if (num != 0) {
                System.out.println(num);
            }
            
        } while (num != 0);
        
        sc.close();
    }
}
