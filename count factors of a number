 Scanner sc = new Scanner(System.in);
        
        int N = sc.nextInt();
        int count = 0;
        
        for (int i = 1; i * i <= N; i++) {
            if (N % i == 0) {
                if (i == N / i)
                    count++; // Perfect square case
                else
                    count += 2; // i and N/i
            }
        }
        
        System.out.println(count);
        sc.close();
    }
}
