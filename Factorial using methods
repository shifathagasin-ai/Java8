import java.util.*;
public class Solution {
   public static long factorial(int n) {
        long fact = 1;
        for(int i = 1; i <= n; i++) {
            fact = fact * i;
        }
        return fact;
    }

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        
        long result = factorial(n); 
        System.out.println(result);
    }
}
